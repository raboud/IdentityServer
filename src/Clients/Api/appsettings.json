{
	"Serilog": {
		"Using": [ "Serilog.Sinks.Graylog" ],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning",
				"Microsoft.AspNetCore.Authentication": "Information"
			}
		},
		"Enrich": [ "FromLogContext" ],
		"WriteTo": [
			{
				"Name": "Graylog",
				"Args": {
					"hostnameOrAddress": "localhost",
					"port": "12201",
					"transportType": "Udp"
				}
			}	],
		"Properties": {
			"Application": "Identity Test API"
		}
	}
}
